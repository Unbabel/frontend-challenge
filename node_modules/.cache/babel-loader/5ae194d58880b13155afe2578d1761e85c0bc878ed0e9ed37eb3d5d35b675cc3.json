{"ast":null,"code":"import CustomCheckbox from \"@/components/CustomCheckbox.vue\";\nimport CustomButton from \"@/components/CustomButton.vue\";\nimport store from \"@/store\";\nexport default {\n  name: \"CardData\",\n  components: {\n    CustomCheckbox,\n    CustomButton\n  },\n  props: {\n    id: Number,\n    voice: String,\n    text: String\n  },\n  data() {\n    return {\n      localId: this.id,\n      localVoice: this.voice,\n      localText: this.text\n    };\n  },\n  methods: {\n    deleteItem() {\n      store.commit(\"deleteMessage\", this.localId);\n    },\n    updateMessageVoice(e) {\n      this.$store.commit(\"updateMessageVoice\", e.target);\n    },\n    updateMessageText(e) {\n      this.$store.commit(\"updateMessageText\", e.target);\n    }\n  }\n};","map":{"version":3,"names":["CustomCheckbox","CustomButton","store","name","components","props","id","Number","voice","String","text","data","localId","localVoice","localText","methods","deleteItem","commit","updateMessageVoice","e","$store","target","updateMessageText"],"sources":["src/components/CardData.vue"],"sourcesContent":["<template>\n  <div class=\"card-data\" data-testid=\"container\">\n    <div class=\"checkbox-container\">\n      <CustomCheckbox />\n    </div>\n    <div class=\"content-container\">\n      <img src=\"@/assets/person.svg\" alt=\"Person icon\" />\n      <div class=\"content\">\n        <input\n          type=\"text\"\n          name=\"title\"\n          @change=\"updateMessageVoice\"\n          :value=\"localVoice\"\n          :id=\"'voice-id-' + localId\"\n          aria-label=\"title\"\n          role=\"input\"\n        />\n        <textarea\n          type=\"text\"\n          name=\"text\"\n          @change=\"updateMessageText\"\n          :value=\"localText\"\n          :id=\"'text-id-' + localId\"\n          aria-label=\"text\"\n          role=\"textarea\"\n        />\n      </div>\n    </div>\n    <div class=\"button-container\">\n      <CustomButton\n        class=\"delete-btn\"\n        @click=\"deleteItem\"\n        label=\"Delete\"\n        icon=\"delete.svg\"\n      />\n    </div>\n  </div>\n</template>\n\n<script>\nimport CustomCheckbox from \"@/components/CustomCheckbox.vue\";\nimport CustomButton from \"@/components/CustomButton.vue\";\nimport store from \"@/store\";\n\nexport default {\n  name: \"CardData\",\n  components: { CustomCheckbox, CustomButton },\n  props: {\n    id: Number,\n    voice: String,\n    text: String,\n  },\n  data() {\n    return {\n      localId: this.id,\n      localVoice: this.voice,\n      localText: this.text,\n    };\n  },\n  methods: {\n    deleteItem() {\n      store.commit(\"deleteMessage\", this.localId);\n    },\n    updateMessageVoice(e) {\n      this.$store.commit(\"updateMessageVoice\", e.target);\n    },\n    updateMessageText(e) {\n      this.$store.commit(\"updateMessageText\", e.target);\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.card-data {\n  display: grid;\n  grid-template-columns: 5% 80% 15%;\n  align-items: flex-start;\n  margin: 1rem 0;\n  padding: 1rem 0.5rem;\n  border-radius: 0.25rem;\n  background-color: #fff;\n  box-shadow: 0px 8px 12px rgba(0, 0, 0, 0.1);\n  &:hover {\n    .button-container {\n      display: block;\n    }\n  }\n  .content-container {\n    display: flex;\n    flex-direction: row;\n    gap: 1rem;\n    align-items: flex-start;\n    .content {\n      display: flex;\n      flex-direction: column;\n      width: 90%;\n      input {\n        font-family: Montserrat;\n        font-size: 16px;\n        font-weight: 600;\n        color: #566074;\n        border: none;\n      }\n      textarea {\n        font-family: Open Sans;\n        font-size: 16px;\n        font-weight: 400;\n        color: #778195;\n        border: none;\n      }\n    }\n  }\n  .checkbox-container {\n    display: flex;\n    justify-content: center;\n    checkbox {\n      width: 1rem;\n      height: 1rem;\n    }\n  }\n  .button-container {\n    display: none;\n    button {\n      bac