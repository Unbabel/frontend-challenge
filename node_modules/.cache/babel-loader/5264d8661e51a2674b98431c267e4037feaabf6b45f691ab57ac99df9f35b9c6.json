{"ast":null,"code":"import CardData from \"@/components/CardData.vue\";\nimport AddCardData from \"@/components/AddCardData.vue\";\nimport store from \"@/store\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    CardData,\n    AddCardData\n  },\n  computed: {\n    messages() {\n      return store.state.messages;\n    }\n  },\n  methods: {\n    addItem() {\n      store.commit(\"addMessage\");\n      store.commit(\"setFormValid\", {\n        isValid: false\n      });\n    },\n    deleteItem(id) {\n      store.commit(\"deleteMessage\", {\n        id\n      });\n    },\n    updateMessageVoice(id, value) {\n      store.commit(\"updateMessageVoice\", {\n        id,\n        value\n      });\n    },\n    updateMessageText(id, value) {\n      store.commit(\"updateMessageText\", {\n        id,\n        value\n      });\n    },\n    setFormValid(isValid) {\n      store.commit(\"setFormValid\", {\n        isValid\n      });\n    }\n  }\n};","map":{"version":3,"names":["CardData","AddCardData","store","name","components","computed","messages","state","methods","addItem","commit","isValid","deleteItem","id","updateMessageVoice","value","updateMessageText","setFormValid"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div v-if=\"messages\">\n      <div v-for=\"item in messages\" :key=\"item.id\">\n        <CardData\n          :item=\"item\"\n          @updateMessageVoice=\"updateMessageVoice\"\n          @updateMessageText=\"updateMessageText\"\n          @deleteItem=\"deleteItem\"\n          @setFormValid=\"setFormValid\"\n        />\n      </div>\n      <AddCardData @addItem=\"addItem\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport CardData from \"@/components/CardData.vue\";\nimport AddCardData from \"@/components/AddCardData.vue\";\nimport store from \"@/store\";\n\nexport default {\n  name: \"HomeView\",\n  components: { CardData, AddCardData },\n  computed: {\n    messages() {\n      return store.state.messages;\n    },\n  },\n  methods: {\n    addItem() {\n      store.commit(\"addMessage\");\n      store.commit(\"setFormValid\", { isValid: false });\n    },\n    deleteItem(id) {\n      store.commit(\"deleteMessage\", { id });\n    },\n    updateMessageVoice(id, value) {\n      store.commit(\"updateMessageVoice\", { id, value });\n    },\n    updateMessageText(id, value) {\n      store.commit(\"updateMessageText\", { id, value });\n    },\n    setFormValid(isValid) {\n      store.commit(\"setFormValid\", { isValid });\n    },\n  },\n};\n</script>\n"],"mappings":"AAkBA,OAAAA,QAAA;AACA,OAAAC,WAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAJ,QAAA;IAAAC;EAAA;EACAI,QAAA;IACAC,SAAA;MACA,OAAAJ,KAAA,CAAAK,KAAA,CAAAD,QAAA;IACA;EACA;EACAE,OAAA;IACAC,QAAA;MACAP,KAAA,CAAAQ,MAAA;MACAR,KAAA,CAAAQ,MAAA;QAAAC,OAAA;MAAA;IACA;IACAC,WAAAC,EAAA;MACAX,KAAA,CAAAQ,MAAA;QAAAG;MAAA;IACA;IACAC,mBAAAD,EAAA,EAAAE,KAAA;MACAb,KAAA,CAAAQ,MAAA;QAAAG,EAAA;QAAAE;MAAA;IACA;IACAC,kBAAAH,EAAA,EAAAE,KAAA;MACAb,KAAA,CAAAQ,MAAA;QAAAG,EAAA;QAAAE;MAAA;IACA;IACAE,aAAAN,OAAA;MACAT,KAAA,CAAAQ,MAAA;QAAAC;MAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}