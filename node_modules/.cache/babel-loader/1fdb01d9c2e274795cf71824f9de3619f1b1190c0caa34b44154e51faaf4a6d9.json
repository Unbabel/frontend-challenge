{"ast":null,"code":"import CustomCheckbox from \"@/components/CustomCheckbox.vue\";\nimport CustomButton from \"@/components/CustomButton.vue\";\nexport default {\n  name: \"CardData\",\n  components: {\n    CustomCheckbox,\n    CustomButton\n  },\n  props: {\n    item: {\n      id: Number,\n      voice: String,\n      text: String\n    }\n  },\n  emits: [\"update:item\"],\n  methods: {\n    deleteItem() {\n      this.$store.commit(\"deleteMessage\", this.id);\n    }\n  },\n  computed: {\n    value: {\n      id: {\n        get() {\n          return this.item.id;\n        },\n        set(value) {\n          this.$emit(\"update:item.id\", value.id);\n        }\n      },\n      voice: {\n        get() {\n          return this.item.voice;\n        },\n        set(value) {\n          this.$emit(\"update:item.voice\", value.voice);\n        }\n      },\n      text: {\n        get() {\n          return this.item.text;\n        },\n        set(value) {\n          this.$emit(\"update:item.text\", value.text);\n        }\n      }\n    }\n  }\n};","map":{"version":3,"names":["CustomCheckbox","CustomButton","name","components","props","item","id","Number","voice","String","text","emits","methods","deleteItem","$store","commit","computed","value","get","set","$emit"],"sources":["src/components/CardData.vue"],"sourcesContent":["<template>\n  <div class=\"card-data\">\n    <div class=\"checkbox-container\">\n      <CustomCheckbox />\n    </div>\n    <div class=\"content-container\">\n      <img src=\"@/assets/person.svg\" alt=\"Person icon\" />\n      <div class=\"content\">\n        <input\n          type=\"text\"\n          name=\"title\"\n          v-model=\"value.voice\"\n          aria-label=\"title\"\n        />\n        <textarea\n          type=\"text\"\n          name=\"text\"\n          v-model=\"value.text\"\n          aria-label=\"text\"\n        />\n      </div>\n    </div>\n    <div class=\"button-container\">\n      <CustomButton @click=\"deleteItem\" label=\"Delete\" icon=\"delete.svg\" />\n    </div>\n  </div>\n</template>\n\n<script>\nimport CustomCheckbox from \"@/components/CustomCheckbox.vue\";\nimport CustomButton from \"@/components/CustomButton.vue\";\n\nexport default {\n  name: \"CardData\",\n  components: { CustomCheckbox, CustomButton },\n  props: {\n    item: {\n      id: Number,\n      voice: String,\n      text: String,\n    },\n  },\n  emits: [\"update:item\"],\n  methods: {\n    deleteItem() {\n      this.$store.commit(\"deleteMessage\", this.id);\n    },\n  },\n  computed: {\n    value: {\n      id: {\n        get() {\n          return this.item.id;\n        },\n        set(value) {\n          this.$emit(\"update:item.id\", value.id);\n        },\n      },\n      voice: {\n        get() {\n          return this.item.voice;\n        },\n        set(value) {\n          this.$emit(\"update:item.voice\", value.voice);\n        },\n      },\n      text: {\n        get() {\n          return this.item.text;\n        },\n        set(value) {\n          this.$emit(\"update:item.text\", value.text);\n        },\n      },\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\">\n.card-data {\n  display: grid;\n  grid-template-columns: 5% 80% 15%;\n  align-items: flex-start;\n  margin: 1rem 0;\n  padding: 1rem 0.5rem;\n  border-radius: 0.25rem;\n  background-color: #fff;\n  box-shadow: 0px 8px 12px rgba(0, 0, 0, 0.1);\n  &:hover {\n    .button-container {\n      display: block;\n    }\n  }\n  .content-container {\n    display: flex;\n    flex-direction: row;\n    gap: 1rem;\n    align-items: flex-start;\n    .content {\n      display: flex;\n      flex-direction: column;\n      width: 90%;\n      input {\n        font-family: Montserrat;\n        font-size: 16px;\n        font-weight: 600;\n        color: #566074;\n        border: none;\n      }\n      textarea {\n        font-family: Open Sans;\n        font-size: 16px;\n        font-weight: 400;\n        color: #778195;\n        border: none;\n      }\n    }\n  }\n  .checkbox-container {\n    display: flex;\n    justify-content: center;\n    checkbox {\n      width: 1rem;\n      height: 1rem;\n    }\n  }\n  .button-container {\n    display: none;\n  }\n}\n</style>\n"],"mappings":"AA6BA,OAAAA,cAAA;AACA,OAAAC,YAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAH,cAAA;IAAAC;EAAA;EACAG,KAAA;IACAC,IAAA;MACAC,EAAA,EAAAC,MAAA;MACAC,KAAA,EAAAC,MAAA;MACAC,IAAA,EAAAD;IACA;EACA;EACAE,KAAA;EACAC,OAAA;IACAC,WAAA;MACA,KAAAC,MAAA,CAAAC,MAAA,uBAAAT,EAAA;IACA;EACA;EACAU,QAAA;IACAC,KAAA;MACAX,EAAA;QACAY,IAAA;UACA,YAAAb,IAAA,CAAAC,EAAA;QACA;QACAa,IAAAF,KAAA;UACA,KAAAG,KAAA,mBAAAH,KAAA,CAAAX,EAAA;QACA;MACA;MACAE,KAAA;QACAU,IAAA;UACA,YAAAb,IAAA,CAAAG,KAAA;QACA;QACAW,IAAAF,KAAA;UACA,KAAAG,KAAA,sBAAAH,KAAA,CAAAT,KAAA;QACA;MACA;MACAE,IAAA;QACAQ,IAAA;UACA,YAAAb,IAAA,CAAAK,IAAA;QACA;QACAS,IAAAF,KAAA;UACA,KAAAG,KAAA,qBAAAH,KAAA,CAAAP,IAAA;QACA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}