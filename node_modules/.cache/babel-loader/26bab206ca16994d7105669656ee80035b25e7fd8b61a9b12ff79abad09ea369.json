{"ast":null,"code":"import CardData from \"@/components/CardData.vue\";\nimport AddCardData from \"@/components/AddCardData.vue\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    CardData,\n    AddCardData\n  },\n  computed: {\n    messages: {\n      get() {\n        return this.$store.state.message;\n      },\n      set(value) {\n        this.$store.commit(\"updateMessage\", value);\n      }\n    }\n  },\n  mounted() {\n    this.$store.dispatch(\"getMessages\");\n  }\n};","map":{"version":3,"names":["CardData","AddCardData","name","components","computed","messages","get","$store","state","message","set","value","commit","mounted","dispatch"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div :v-if=\"this.messages\">\n      <div v-for=\"item in this.messages\" :key=\"item.id\">\n        <CardData :id=\"item.id\" :voice=\"item.voice\" :text=\"item.text\" />\n      </div>\n      <AddCardData />\n    </div>\n  </div>\n</template>\n\n<script>\nimport CardData from \"@/components/CardData.vue\";\nimport AddCardData from \"@/components/AddCardData.vue\";\n\nexport default {\n  name: \"HomeView\",\n  components: { CardData, AddCardData },\n  computed: {\n    messages: {\n      get() {\n        return this.$store.state.message;\n      },\n      set(value) {\n        this.$store.commit(\"updateMessage\", value);\n      },\n    },\n  },\n  mounted() {\n    this.$store.dispatch(\"getMessages\");\n  },\n};\n</script>\n"],"mappings":"AAYA,OAAAA,QAAA;AACA,OAAAC,WAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IAAAH,QAAA;IAAAC;EAAA;EACAG,QAAA;IACAC,QAAA;MACAC,IAAA;QACA,YAAAC,MAAA,CAAAC,KAAA,CAAAC,OAAA;MACA;MACAC,IAAAC,KAAA;QACA,KAAAJ,MAAA,CAAAK,MAAA,kBAAAD,KAAA;MACA;IACA;EACA;EACAE,QAAA;IACA,KAAAN,MAAA,CAAAO,QAAA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}