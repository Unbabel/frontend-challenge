{"ast":null,"code":"import UserName from \"./UserName.vue\";\nexport default {\n  components: {\n    UserName\n  },\n  data() {\n    return {\n      first: \"John\",\n      last: \"Doe\"\n    };\n  }\n};","map":{"version":3,"names":["UserName","components","data","first","last"],"sources":["src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"home\">\n    <div :v-if=\"this.messages\">\n      <div v-for=\"(item, index) in this.messages\" :key=\"item.id\">\n        <CardData\n          v-model:voice=\"item[index].voice\"\n          v-model:text=\"item[index].text\"\n        />\n      </div>\n      <AddCardData />\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapState } from \"vuex\";\nimport CardData from \"@/components/CardData.vue\";\nimport AddCardData from \"@/components/AddCardData.vue\";\n\nexport default {\n  name: \"HomeView\",\n  components: { CardData, AddCardData },\n  computed: mapState([\"messages\"]),\n  mounted() {\n    this.$store.dispatch(\"getMessages\");\n  },\n};\n</script>\n\n<script>\nimport UserName from \"./UserName.vue\";\n\nexport default {\n  components: { UserName },\n  data() {\n    return {\n      first: \"John\",\n      last: \"Doe\",\n    };\n  },\n};\n</script>\n\n<template>\n  <h1>{{ first }} {{ last }}</h1>\n  <UserName v-model:first-name=\"first\" v-model:last-name=\"last\" />\n</template>\n"],"mappings":"AA8BA,OAAAA,QAAA;AAEA;EACAC,UAAA;IAAAD;EAAA;EACAE,KAAA;IACA;MACAC,KAAA;MACAC,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}